{
	"compilerOptions": {
	  "baseUrl": "src",
	  "paths": {
		"@components/*": ["components/*"],
		"@utils/*": ["utils/*"],
		"@store/*": ["store/*"],
		"@api": ["plugins/axios"],
		"@assets/*": ["assets/*"],
		"@pages/*": ["pages/*"]
	  },
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "jsx": "react-jsx",
	},
	"include": ["src"]
  }